 <script type="text/javascript" language="javascript">

 $(document).ready(function(){
 	

 	$('#short').click(function(){

 		var url=$('#url').val();
 		$.ajax({
 			url:'../main/generate',
 			type:'POST',
 			data:{url:url},
 			success : function(json)
 			{
 				$('#req_res').html("localhost:3000/?"+json["result"]);
 			} ,

 		});
 	});

 });
 </script>

<h1>Welcome to Our Site</h1>

<p>Get Your URL Shortend By One Click !!</p>

<table width="100%">
	<tr>
		<td style="text-align:center">
			<div class="row">
				<strong>URL :  </strong>
				<input type="text" id="url" name="url" size="70"/>
				<input type="button" id="short" name="short" value="Short It !!"/>
			</div>
		</td>	
	</tr>
	
	<tr>
		<td style="text-align:center">
			<br/>
			<h4><font color="green">Your Short URL Is : </font><br><br>
				<div id="req_res" name="req_res"></div>
			</h4>
		</td>	
	</tr>
</table>


